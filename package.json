{
  "name": "editor-emendas-lit",
  "version": "0.0.8",
  "main": "dist/my-element.es.js",
  "exports": {
    ".": "./dist/my-element.es.js"
  },
  "types": "types/my-element.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "prepublish": "rimraf dist && rollup -c rollup.config.dist.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "lint": "eslint --ext .ts . --ignore-path .gitignore --ignore-path .eslintignore && prettier \"**/*.ts\" --check --ignore-path .gitignore --ignore-path .eslintignore",
    "format": "eslint --ext .ts . --fix --ignore-path .gitignore --ignore-path .eslintignore && prettier \"**/*.ts\" --write --ignore-path .gitignore --ignore-path .eslintignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.0.0-beta.73",
    "lit": "^2.2.7",
    "rollup-plugin-import-css": "^3.0.3"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^7.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "happy-dom": "^2.54.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.4",
    "vite": "^2.9.0",
    "vitest": "^0.8.1"
  },
  "customElements": "custom-elements.json",
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
